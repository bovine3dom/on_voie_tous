{
  "manifest_version": 3,
  "name": "On Voie Tous",
  "version": "0.1",
  "permissions": [],
  "icons": {
    "32": "logo_32.png",
    "64": "logo_64.png"
  },
  "host_permissions": [
    "https://www.garesetconnexions.sncf/*"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "on_voie_tous@bovine3dom",
      "data_collection_permissions": {"required": ["none"]}
    }
  },
  "content_scripts": [
    {
      "matches": ["https://www.garesetconnexions.sncf/*"],
      "js": ["content.user.js"],
      "run_at": "document_start",
      "world": "MAIN"
    }
  ]
}
